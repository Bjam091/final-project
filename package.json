  "bugs": {
    "url": "https://github.com/NimaBoscarino/stigma/issues"
  },
  "homepage": "https://github.com/NimaBoscarino/stigma#readme",
  "dependencies": {
    "styled-components": "^4.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@storybook/addon-actions": "^5.2.6",
    "@storybook/addon-links": "^5.2.6",
    "@storybook/addons": "^5.2.6",
    "@storybook/react": "^5.2.6",
