  "dependencies": {
    "leaflet": "^1.6.0",
    "leaflet.markercluster": "^1.4.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-leaflet": "^2.6.0",
    "react-leaflet-markercluster": "^2.0.0-rc3"
  }
