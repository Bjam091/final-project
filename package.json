{
  "name": "stigma",
  "version": "1.0.0",
  "description": "A boilerplate project for anyone interested in making a project that uses React and Rails.",
  "main": "index.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "engines": {
    "node": "6.3.1"
